{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/proxy/:match/:url*", "destination": "https://:match/:url*" },
    {
      "source": "/proxy/:match/:url*/",
      "destination": "https://:match/:url*/"
    },
    {
      "source": "/httpproxy/:match/:url*",
      "destination": "http://:match/:url*"
    },
    {
      "source": "/httpproxy/:match/:url*/",
      "destination": "http://:match/:url*/"
    }
  ]
}
